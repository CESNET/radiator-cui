sub
{
 my $request = ${$_[0]};
 if ($request->code() eq 'Access-Request')
 {
  my @cui = $request->get_attr("Chargeable-User-Identity");
  if ( $#cui == 0 )
  {
  	$request->change_attr("Chargeable-User-Identity", "");
  }
  my $opname = &main::getVariable('CUI_Operator_Name');
  if ( $opname ) 
  {
   $request->change_attr("Operator-Name", $opname );
  }
 } 
}
